{
  "cleanUrls": true,
  "trailingSlash": false,
  "redirects": [
    {
      "source": "/ko/w/archive",
      "destination": "/ko/w",
      "permanent": true
    },
    {
      "source": "/w/archive",
      "destination": "/en/w",
      "permanent": true
    },
    {
      "source": "/w/:path(.*)",
      "destination": "/en/w/:path",
      "permanent": true
    },
    {
      "source": "/ko/r",
      "destination": "/ko/r/000000",
      "permanent": true
    },
    {
      "source": "/r",
      "destination": "/en/r/000000",
      "permanent": true
    }
  ]
}
