{
  "extends": "@docusaurus/tsconfig",
  "compilerOptions": {
    "baseUrl": ".",
    "types": [
      "node",
      "react",
      "react-dom",
      "@docusaurus/module-type-aliases",
      "@docusaurus/theme-classic"
    ],
    "lib": ["dom", "esnext"],
    "paths": {
      "@site/*": ["./*"]
    }
  },
  "exclude": ["node_modules", "build"]
}
