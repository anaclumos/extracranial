{
  "name": "cho-sh",
  "version": "1.0.0",
  "license": "MIT",
  "scripts": {
    "docusaurus": "docusaurus",
    "start": "$npm_execpath preprocess && docusaurus start",
    "build": "$npm_execpath clear && $npm_execpath preprocess && docusaurus build",
    "swizzle": "docusaurus swizzle",
    "deploy": "$npm_execpath preprocess && docusaurus deploy",
    "serve": "docusaurus serve",
    "clear": "docusaurus clear && rm -rf 'blog' && rm -rf 'docs' && rm -f '*.config.js' && rm -rf 'i18n/**/*.md' && cp tools/안녕하세요.md i18n/ko/docusaurus-plugin-content-docs/current/Hey.md && rm -rf 'i18n/**/*.png' && rm -rf 'i18n/**/*.svg' && rm -rf 'i18n/**/*.jpg' && rm -rf 'i18n/**/*.jpeg'",
    "clean": "$npm_execpath clear",
    "write-translations": "docusaurus write-translations && docusaurus write-translations --locale ko",
    "write-heading-ids": "docusaurus write-heading-ids",
    "preprocess": "$npm_execpath pack-post & $npm_execpath format & $npm_execpath wikilinks & $npm_execpath backlinks & $npm_execpath swc && $npm_execpath figcaption",
    "format": "prettier --config .prettierrc -w '**/*.{ts,tsx,json,md,mdx}'",
    "ko": "$npm_execpath start --locale ko",
    "dev": "$npm_execpath start",
    "hex": "python3 tools/remove-hexmark.py",
    "pack-post": "python3 tools/pack-post.py",
    "wikilinks": "python3 tools/wikilinks-to-mdlinks.py",
    "backlinks": "python3 tools/process-backlinks.py",
    "figcaption": "python3 tools/img-alt-to-figcaption.py",
    "swc": "swc docusaurus.config.ts -o docusaurus.config.js && swc config/*.ts -d config/"
  },
  "dependencies": {
    "@docusaurus/core": "^2.2.0",
    "@docusaurus/plugin-client-redirects": "^2.2.0",
    "@docusaurus/plugin-google-gtag": "^2.2.0",
    "@docusaurus/plugin-pwa": "^2.2.0",
    "@docusaurus/preset-classic": "^2.2.0",
    "@docusaurus/theme-common": "^2.2.0",
    "@docusaurus/types": "^2.2.0",
    "@giscus/react": "^2.2.0",
    "@mdx-js/react": "^1.6.22",
    "@swc/cli": "^0.1.57",
    "@swc/core": "^1.3.11",
    "clsx": "^1.2.1",
    "cobe": "^0.6.2",
    "prism-react-renderer": "^1.3.5",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-force-graph": "^1.41.14",
    "react-spring": "^9.5.5",
    "rehype-katex": "^5.0.0",
    "remark-math": "^3.0.1",
    "swc-loader": "^0.2.3",
    "three": "^0.146.0",
    "ts-node": "^10.9.1"
  },
  "devDependencies": {
    "@docusaurus/module-type-aliases": "^2.2.0",
    "@tsconfig/docusaurus": "^1.0.6",
    "@types/node": "^18.11.7",
    "@typescript-eslint/eslint-plugin": "^5.0.0",
    "@typescript-eslint/parser": "^5.41.0",
    "babel-eslint": "^10.1.0",
    "eslint": "^8.0.1",
    "eslint-config-airbnb": "^19.0.4",
    "eslint-config-standard-with-typescript": "latest",
    "eslint-plugin-import": "^2.25.2",
    "eslint-plugin-jsx-a11y": "^6.6.1",
    "eslint-plugin-n": "^15.0.0",
    "eslint-plugin-promise": "^6.0.0",
    "eslint-plugin-react": "latest",
    "eslint-plugin-react-hooks": "^4.6.0",
    "husky": "^8.0.1",
    "lint-staged": "^13.0.3",
    "prettier": "^2.7.1",
    "typescript": "*"
  },
  "browserslist": {
    "production": [
      ">0.5%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "main": "index.js",
  "repository": "https://github.com/anaclumos/extracranial.git",
  "author": "Sunghyun Cho <mail@chosunghyun.com>",
  "packageManager": "yarn@3.2.3",
  "lint-staged": {
    "**/*.{ts,tsx,json,md,mdx,js,jsx}": [
      "prettier --config .prettierrc"
    ]
  }
}
