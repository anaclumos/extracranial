{
  "name": "cho-sh",
  "version": "1.0.0",
  "license": "MIT",
  "scripts": {
    "docusaurus": "docusaurus",
    "start": "$npm_execpath run preprocess && docusaurus start",
    "build": "$npm_execpath run swc && docusaurus build",
    "swizzle": "docusaurus swizzle",
    "deploy": "$npm_execpath run preprocess && docusaurus deploy",
    "serve": "docusaurus serve",
    "clear": "docusaurus clear && rm -rf 'blog' && rm -rf 'docs' && rm -f '**/*.config.js' && rm -f '**/*.config.js.map' && rm -rf 'i18n /**/*.md' && cp tools/안녕하세요.md i18n/ko/docusaurus-plugin-content-docs/current/Hey.md && rm -rf 'i18n /**/*.png' && rm -rf 'i18n /**/*.svg' && rm -rf 'i18n /**/*.jpg' && rm -rf 'i18n /**/*.jpeg'",
    "clean": "$npm_execpath run clear",
    "write-translations": "docusaurus write-translations && docusaurus write-translations --locale ko",
    "write-heading-ids": "docusaurus write-heading-ids",
    "preprocess": "$npm_execpath run sanitize && $npm_execpath run pack-post && $npm_execpath run wikilinks && $npm_execpath run backlinks && $npm_execpath run swc && $npm_execpath run figcaption",
    "format": "prettier --config .prettierrc -w '**/*.{ts,tsx,json,md,mdx,css,scss}' >> format.log",
    "ko": "$npm_execpath run start --locale ko",
    "dev": "$npm_execpath run start",
    "ko:instant": "docusaurus start --locale ko",
    "dev:instant": "docusaurus start",
    "sanitize": "python3 tools/sanitize.py",
    "pack-post": "python3 tools/pack-post.py",
    "wikilinks": "python3 tools/wikilinks-to-mdlinks.py",
    "backlinks": "python3 tools/process-backlinks.py",
    "figcaption": "python3 tools/img-alt-to-figcaption.py",
    "swc": "swc docusaurus.config.ts -o docusaurus.config.js && swc config/*.ts -d config/",
    "readme": "cp tools/README.src.md README.md",
    "scc": "echo \"\n## Stats\n\" >> README.md && echo \"```\" >> README.md && scc . >> README.md && echo \"```\" >> README.md"
  },
  "dependencies": {
    "@docusaurus/core": "^2.3.0",
    "@docusaurus/plugin-client-redirects": "^2.3.0",
    "@docusaurus/plugin-google-gtag": "^2.3.0",
    "@docusaurus/plugin-pwa": "^2.3.0",
    "@docusaurus/preset-classic": "^2.3.0",
    "@docusaurus/theme-common": "^2.3.0",
    "@docusaurus/types": "^2.3.0",
    "@giscus/react": "^2.2.6",
    "@mdx-js/react": "^1.6.22",
    "@swc/cli": "^0.1.59",
    "@swc/core": "^1.3.29",
    "clsx": "^1.2.1",
    "cobe": "^0.6.3",
    "iframe-resizer-react": "^1.1.0",
    "prism-react-renderer": "^1.3.5",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-force-graph": "^1.41.18",
    "react-spring": "^9.6.1",
    "react-wrap-balancer": "^0.4.0",
    "rehype-katex": "^5.0.0",
    "remark-math": "^3.0.1",
    "swc-loader": "^0.2.3",
    "three": "^0.149.0",
    "ts-node": "^10.9.1",
    "webpack": "^5.75.0"
  },
  "devDependencies": {
    "@docusaurus/module-type-aliases": "^2.3.0",
    "@tsconfig/docusaurus": "^1.0.6",
    "@types/node": "^18.11.18",
    "@typescript-eslint/eslint-plugin": "^5.49.0",
    "@typescript-eslint/parser": "^5.49.0",
    "babel-eslint": "^10.1.0",
    "eslint": "^8.32.0",
    "eslint-config-airbnb": "^19.0.4",
    "eslint-config-standard-with-typescript": "^23.0.0",
    "eslint-plugin-import": "^2.27.5",
    "eslint-plugin-jsx-a11y": "^6.7.1",
    "eslint-plugin-n": "^15.6.1",
    "eslint-plugin-promise": "^6.1.1",
    "eslint-plugin-react": "^7.32.1",
    "eslint-plugin-react-hooks": "^4.6.0",
    "prettier": "^2.8.3",
    "typescript": "^4.9.4"
  },
  "browserslist": {
    "production": [
      ">0.5%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "main": "index.js",
  "repository": "https://github.com/anaclumos/extracranial.git",
  "author": "Sunghyun Cho <mail@chosunghyun.com>",
  "packageManager": "yarn@3.2.3"
}
