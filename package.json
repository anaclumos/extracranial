    "@docusaurus/theme-mermaid": "^3.9.2",
    "@giscus/react": "^3.1.0",
    "@vercel/analytics": "^1.5.0",
    "@vercel/speed-insights": "^1.3.1",
    "clsx": "^2.1.1",
    "cobe": "^0.6.4",
    "corner-smoothing": "^0.1.5",
