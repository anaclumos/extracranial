{
  "name": "cho-sh",
  "version": "1.0.0",
  "license": "MIT",
  "scripts": {
    "docusaurus": "docusaurus",
    "start": "$npm_execpath run prepare && docusaurus start",
    "build": "$npm_execpath run prepare && docusaurus build && $npm_execpath run redirects",
    "redirects": "cp _redirects build/_redirects",
    "swizzle": "docusaurus swizzle",
    "deploy": "$npm_execpath run build && wrangler pages publish ./build --commit-dirty=true --project-name=memex",
    "serve": "docusaurus serve",
    "clear": "docusaurus clear && rm -rf 'blog' && rm -rf 'docs' && rm -f '**/*.config.js' && rm -f '**/*.config.js.map' && rm -rf 'i18n /**/*.md' && cp tools/안녕하세요.md i18n/ko/docusaurus-plugin-content-docs/current/Hey.md && rm -rf 'i18n /**/*.png' && rm -rf 'i18n /**/*.svg' && rm -rf 'i18n /**/*.jpg' && rm -rf 'i18n /**/*.jpeg'",
    "clean": "$npm_execpath run clear",
    "write-translations": "docusaurus write-translations && docusaurus write-translations --locale ko",
    "write-heading-ids": "docusaurus write-heading-ids",
    "prepare": "$npm_execpath run clear && $npm_execpath run sanitize && $npm_execpath run bundle && $npm_execpath run wikilinks && $npm_execpath run backlinks && $npm_execpath run swc && $npm_execpath run figcaption && $npm_execpath run readme && $npm_execpath run scc && $npm_execpath run format",
    "format": "prettier --config .prettierrc -w '**/*.{ts,tsx,json,md,mdx,css,scss,html,yml,yaml,js,jsx,svg}'",
    "ko": "$npm_execpath run start --locale ko",
    "dev": "$npm_execpath run start",
    "ko:instant": "docusaurus start --locale ko",
    "dev:instant": "docusaurus start",
    "sanitize": "python3 tools/sanitize.py",
    "bundle": "python3 tools/bundle.py",
    "wikilinks": "python3 tools/wikilinks-to-mdlinks.py",
    "backlinks": "python3 tools/process-backlinks.py",
    "figcaption": "python3 tools/img-alt-to-figcaption.py",
    "swc": "swc docusaurus.config.ts -o docusaurus.config.js && swc config/*.ts -d config/",
    "readme": "cp tools/README.src.md README.md",
    "scc": "echo \"\n## Stats\n\" >> README.md && echo \"```\" >> README.md && scc . >> README.md && echo \"```\" >> README.md"
  },
  "dependencies": {
    "@docusaurus/core": "^2.4.0",
    "@docusaurus/plugin-client-redirects": "^2.4.0",
    "@docusaurus/plugin-google-gtag": "^2.4.0",
    "@docusaurus/plugin-pwa": "^2.4.0",
    "@docusaurus/preset-classic": "^2.4.0",
    "@docusaurus/theme-common": "^2.4.0",
    "@docusaurus/types": "^2.4.0",
    "@giscus/react": "^2.2.8",
    "@mdx-js/react": "^1.6.22",
    "@swc/cli": "^0.1.62",
    "@swc/core": "^1.3.49",
    "clsx": "^1.2.1",
    "cobe": "^0.6.3",
    "iframe-resizer-react": "^1.1.0",
    "prism-react-renderer": "^1.3.5",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-force-graph": "^1.42.1",
    "react-spring": "^9.7.1",
    "react-wrap-balancer": "^0.4.0",
    "rehype-katex": "^5.0.0",
    "remark-math": "^3.0.1",
    "swc-loader": "^0.2.3",
    "three": "^0.151.3",
    "ts-node": "^10.9.1",
    "webpack": "^5.78.0"
  },
  "devDependencies": {
    "@cloudflare/workers-types": "^4.20230404.0",
    "@docusaurus/module-type-aliases": "^2.4.0",
    "@prettier/plugin-xml": "^2.2.0",
    "@tsconfig/docusaurus": "^1.0.7",
    "@types/node": "^18.15.11",
    "@typescript-eslint/eslint-plugin": "^5.58.0",
    "@typescript-eslint/parser": "^5.58.0",
    "babel-eslint": "^10.1.0",
    "eslint": "^8.38.0",
    "eslint-config-airbnb": "^19.0.4",
    "eslint-config-standard-with-typescript": "^34.0.1",
    "eslint-plugin-import": "^2.27.5",
    "eslint-plugin-jsx-a11y": "^6.7.1",
    "eslint-plugin-n": "^15.7.0",
    "eslint-plugin-promise": "^6.1.1",
    "eslint-plugin-react": "^7.32.2",
    "eslint-plugin-react-hooks": "^4.6.0",
    "prettier": "^2.8.7",
    "typescript": "^5.0.4",
    "wrangler": "2.15.0"
  },
  "browserslist": {
    "production": [
      ">0.5%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "main": "index.js",
  "repository": "https://github.com/anaclumos/extracranial.git",
  "author": "Sunghyun Cho <mail@chosunghyun.com>",
  "packageManager": "yarn@3.2.3"
}
