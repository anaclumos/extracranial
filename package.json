{
  "author": "Sunghyun Cho <hey@cho.sh>",
  "dependencies": {
    "@docusaurus/core": "^3.8.0",
    "@docusaurus/faster": "^3.8.0",
    "@docusaurus/plugin-vercel-analytics": "^3.8.0",
    "@docusaurus/preset-classic": "^3.8.0",
    "@docusaurus/theme-mermaid": "^3.8.0",
    "@giscus/react": "^3.1.0",
    "@radix-ui/react-dialog": "^1.1.14",
    "@radix-ui/react-label": "^2.1.7",
    "@radix-ui/react-popover": "^1.1.14",
    "@radix-ui/react-slot": "^1.2.3",
    "@vercel/analytics": "^1.5.0",
    "@vercel/speed-insights": "^1.2.0",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "cobe": "^0.6.3",
    "lucide-react": "^0.511.0",
    "react": "^19.1.0",
    "react-dom": "^19.1.0",
    "react-player": "^2.16.0",
    "react-spring": "^10.0.1",
    "rehype-katex": "^7.0.1",
    "remark-math": "^6.0.0",
    "tailwind-merge": "^3.3.0",
    "vaul": "^1.1.2"
  },
  "devDependencies": {
    "@docusaurus/tsconfig": "^3.8.0",
    "@types/node": "^22.15.24",
    "@types/react": "^19.1.6",
    "@types/react-dom": "^19.1.5",
    "prettier": "^3.5.3"
  },
  "license": "MIT",
  "name": "extracranial",
  "repository": "https://github.com/anaclumos/extracranial.git",
  "scripts": {
    "build": "bun ready && bun run format && NODE_OPTIONS=\"--max-old-space-size=7680\" docusaurus build --no-minify",
    "dev": "docusaurus start -l ko",
    "format": "prettier -w '**/*.{ts,tsx,json,md,mdx,css,scss,html,yml,yaml,mts,mjs,cts,cjs,js,jsx,xml}' --log-level silent",
    "readme": "cp md/README.src.md README.md && printf \"\n\n## Last updated \n\n$(date)\n\" >> README.md && printf '\n## Stats\n' >> README.md && printf '\n```\n' >> README.md && scc . >> README.md && printf '```\n' >> README.md",
    "ready": "env PYTHONUTF8=1 PYTHONHASHSEED=0 PYTHONDONTWRITEBYTECODE=1 python3 -OO preprocess.py",
    "save": "bun ready && bun run readme && bun run format && git add . && git commit -m 'wrote something' && git push",
    "start": "docusaurus serve",
    "update-post": "rm -rf i18n/ko/docusaurus-plugin-content-blog/2025-05-20-incl-rebuild && cp -R posts/2025-05-20-incl-rebuild i18n/ko/docusaurus-plugin-content-blog/2025-05-20-incl-rebuild && mv i18n/ko/docusaurus-plugin-content-blog/2025-05-20-incl-rebuild/ko.md i18n/ko/docusaurus-plugin-content-blog/2025-05-20-incl-rebuild/index.md",
    "poll-post": "while true; do bun update-post; sleep 10; done"
  },
  "trustedDependencies": [
    "@swc/core",
    "core-js",
    "core-js-pure"
  ],
  "version": "1.0.0"
}
