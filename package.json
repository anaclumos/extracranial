    "react-dom": "^19.1.1",
    "rehype-katex": "^7.0.1",
    "remark-math": "^6.0.0"
  },
  "devDependencies": {
    "@biomejs/biome": "2.3.10",
    "@docusaurus/module-type-aliases": "^3.9.2",
    "@docusaurus/tsconfig": "^3.9.2",
    "@docusaurus/types": "^3.9.2",
    "@tailwindcss/postcss": "^4.1.18",
    "@types/node": "^24.2.0",
    "@types/react": "^19.1.9",
    "@types/react-dom": "^19.1.7",
    "autoprefixer": "^10.4.23",
    "lefthook": "^2.0.13",
    "postcss": "^8.5.6",
    "tailwindcss": "^4.1.18",
    "typescript": "^5.9.2",
    "ultracite": "7.0.3"
  },
  "license": "MIT",
  "name": "extracranial",
  "repository": "https://github.com/anaclumos/extracranial.git",
  "scripts": {
    "build": " bun ready &&  bun run format && docusaurus build",
    "dev": "docusaurus start -l ko",
    "format": "bunx prettier -w '**/*.{md,mdx}' --log-level silent",
    "readme": "cp md/README.src.md README.md && printf \"\n\n## Last updated \n\n$(date)\n\" >> README.md && printf '\n## Stats\n' >> README.md && printf '\n```\n' >> README.md && scc . >> README.md && printf '```\n' >> README.md",
    "ready": "bun preprocess.ts",
    "save": "bun ready && bun run readme && bun run format && git add . && git commit -m \"$(git diff --cached -- . ':!README.md' ':!src/data/*.json' | head -c 200000 | claude -p 'Write a git commit message for these changes. Use lowercase, imperative mood, max 50 chars. Output only the message, no quotes.')\" && git push",
    "start": "docusaurus serve",
    "prepare": "lefthook install"
  },
  "trustedDependencies": [
    "@swc/core",
    "@vercel/speed-insights",
    "core-js",
    "core-js-pure"
  ],
  "version": "1.0.0"
}
