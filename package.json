{
  "name": "cho-sh",
  "version": "1.0.0",
  "license": "MIT",
  "scripts": {
    "docusaurus": "docusaurus",
    "start": "yarn build:config && docusaurus start",
    "build": "yarn clear && yarn build:config && docusaurus build",
    "swizzle": "docusaurus swizzle",
    "deploy": "yarn build:config && docusaurus deploy",
    "serve": "docusaurus serve",
    "clear": "docusaurus clear && rm -rf 'blog' && rm -rf 'docs' && rm -f '*.config.js' && rm -rf 'i18n/**/*.md' && cp tools/안녕하세요.md i18n/ko/docusaurus-plugin-content-docs/current/Hey.md && rm -rf 'i18n/**/*.png' && rm -rf 'i18n/**/*.svg' && rm -rf 'i18n/**/*.jpg' && rm -rf 'i18n/**/*.jpeg'",
    "clean": "yarn clear",
    "compile": "swc docusaurus.config.ts -o docusaurus.config.js && swc config/*.ts -d config/",
    "write-translations": "docusaurus write-translations && docusaurus write-translations --locale ko",
    "write-heading-ids": "docusaurus write-heading-ids",
    "build:plugin": "yarn workspace plugin-blog-index build",
    "build:config": "yarn pack-post && yarn wikilinks-to-mdlinks && yarn process-backlinks && yarn img-alt-to-figcaption && yarn compile",
    "refresh": "yarn build:config",
    "format": "prettier --config .prettierrc -w '**/*.{ts,tsx,json,md,mdx}'",
    "ko": "yarn start --locale ko",
    "dev": "yarn start",
    "hex": "python3 tools/remove-hexmark.py",
    "pack-post": "python3 tools/pack-post.py",
    "process-backlinks": "python3 tools/process-backlinks.py",
    "wikilinks-to-mdlinks": "python3 tools/wikilinks-to-mdlinks.py",
    "img-alt-to-figcaption": "python3 tools/img-alt-to-figcaption.py"
  },
  "dependencies": {
    "@docusaurus/core": "^2.2.0",
    "@docusaurus/plugin-client-redirects": "^2.2.0",
    "@docusaurus/plugin-google-gtag": "^2.2.0",
    "@docusaurus/plugin-pwa": "^2.2.0",
    "@docusaurus/preset-classic": "^2.2.0",
    "@docusaurus/theme-common": "^2.2.0",
    "@docusaurus/types": "^2.2.0",
    "@giscus/react": "^2.2.0",
    "@mdx-js/react": "^1.6.22",
    "@swc/cli": "^0.1.57",
    "@swc/core": "^1.3.11",
    "clsx": "^1.2.1",
    "cobe": "^0.6.2",
    "prism-react-renderer": "^1.3.5",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-force-graph": "^1.41.14",
    "react-spring": "^9.5.5",
    "rehype-katex": "^5.0.0",
    "remark-math": "^3.0.1",
    "swc-loader": "^0.2.3",
    "three": "^0.146.0",
    "ts-node": "^10.9.1"
  },
  "devDependencies": {
    "@docusaurus/module-type-aliases": "^2.2.0",
    "@tsconfig/docusaurus": "^1.0.6",
    "@types/node": "^18.11.7",
    "@typescript-eslint/eslint-plugin": "^5.41.0",
    "@typescript-eslint/parser": "^5.41.0",
    "eslint": "^8.26.0",
    "eslint-config-airbnb": "^19.0.4",
    "eslint-plugin-import": "^2.26.0",
    "eslint-plugin-jsx-a11y": "^6.6.1",
    "eslint-plugin-react": "^7.31.10",
    "eslint-plugin-react-hooks": "^4.6.0",
    "prettier": "^2.7.1",
    "typescript": "^4.8.4"
  },
  "browserslist": {
    "production": [
      ">0.5%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "main": "index.js",
  "repository": "https://github.com/anaclumos/extracranial.git",
  "author": "Sunghyun Cho <mail@chosunghyun.com>",
  "packageManager": "yarn@3.2.3"
}
